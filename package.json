{
  "name": "dsv_importer",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://vdl.sci.utah.edu/dsv_importer/",
  "dependencies": {
    "@material-ui/core": "^4.8.1",
    "@material-ui/icons": "^4.5.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^13.13.5",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "axios": "^0.19.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "d3": "^5.14.2",
    "husky": "^3.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "typeface-roboto": "^0.0.75",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:prod": "CI=true react-scripts test",
    "test:coverage:prod": "CI=true react-scripts test --coverage",
    "eject": "react-scripts eject",
    "commit": "git-cz"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:coverage:prod",
      "pre-push": "npm run test:coverage:prod"
    }
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bit": {
    "env": {
      "compiler": {
        "bit.envs/compilers/react-typescript@3.1.27": {
          "rawConfig": {
            "tsConfig": {
              "strict": "false",
              "lib": [
                "dom",
                "dom.iterable",
                "esnext",
                "es2020",
                "ES2017.Object"
              ]
            }
          }
        }
      }
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  },
  "devDependencies": {
    "@types/d3": "^5.7.2"
  }
}
